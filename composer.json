{
    "name": "kikwik/iptraceable-listener-bundle",
    "description": "Listener for gedmo/doctrine-extension IpTraceable feature",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Davide Scarpato",
            "email": "davide@kikwik.it"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.4|^5.0",
        "stof/doctrine-extensions-bundle": "^1.4",
        "gedmo/doctrine-extensions": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Kikwik\\IpTraceableListenerBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4|^5.0",
        "nyholm/symfony-bundle-test": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Kikwik\\IpTraceableListenerBundle\\Tests\\": "tests/"
        }
    }
}
